=link_to 'back to the dictionary', dictionaries_path, class: 'btn btn-outline'

=form_with model: @user, class: 'container', local: true do |f|
  .form_group
    =f.label :name 
    =f.text_field :name
  .form-group
    =f.label :email
    =f.text_field :email
  .form-group
    =f.label :password
    =f.password_field :password_digest

  .form-group
    =f.submit nil, class: 'btn btn-outline-primary'